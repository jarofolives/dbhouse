<script>
    $(function () {
        $(".main_svg").load('https://www.goldmedalsquared.com/images/svg.svg');
        $.ajaxSetup({
            headers: {
                'X-CSRF-Token': $('meta[name="_token"]').attr('content')
            }
        });


        $('.jsNavBt').click(function(){
            var thisParent = $(this).closest('.jsMobileNavParent');
            thisParent.toggleClass('show_menu');
            thisParent.find('.jsNavShow').collapse('toggle');
        });

        var scrollTop = $(document).scrollTop();
        $(window).scroll(function() {
            var headerHeight = $('.sticky_header').outerHeight() - 20;
            var scrollTopPosition = $(document).scrollTop();
            if (scrollTopPosition > headerHeight){
                $('.sticky_header').addClass('hide_header');
            }
            else {
                $('.sticky_header').removeClass('hide_header');
            }
            if (scrollTopPosition > scrollTop){
                $('.sticky_header').removeClass('show_header');
            }
            else {
                $('.sticky_header').addClass('show_header');
            }
            scrollTop = $(document).scrollTop();
        });
        // full slider
        var swiperFullSlider = new Swiper('.jsSlider', {
            pagination: '.swiper-pagination',
            paginationClickable: true,
            nextButton: '.swiper-button-next',
            prevButton: '.swiper-button-prev',
        });
        // END full slider

        
$('.play-video').click(function(){
  var videoId = $(this).data('videoId');
  bootbox.dialog({
    className: 'modal-wider wider force-no-transform',
    message: '<div class="bootbox-preview" style="min-height: 200px; margin-top: 30px; border: 1px solid #f0f0f0;"></div>',
    buttons: {
      ok: {
        label: "Close"
      }
    }
  }).on('shown.bs.modal', function() {
    $.getJSON('https://www.goldmedalsquared.com/courses/video/' + videoId, {}, function(result){
      if (result.id) {
        function createPlayer(response) {
          var player = '<div class="invalid-id">Invalid ID</div>';
          if (response.id && response.security_token) {
            var ratio = response.width / response.height;
            var height = $('.bootbox-preview').width() / ratio;
            player = '<div class="video"><iframe class="sproutvideo-player" type="text/html" src="https://videos.sproutvideo.com/embed/' + response.id + '/' + response.security_token + '?type=hd&noBigPlay=true&showcontrols=true" width="100%" height="' + height + '" frameborder="0" allowfullscreen></iframe></div>';
          }
          $('.bootbox .bootbox-preview').html(player);
        }
        createPlayer(result);
      }
    });
  }).on('hide.bs.modal', function(){
    $('.bootbox .bootbox-preview').empty();
  });

});
$('.lessons-list .show-all').click(function(){
  $(this).closest('.lessons-list').find('li.d-none').addClass('dd-none').removeClass('d-none');
  $(this).addClass('d-none');
  $(this).parent().find('.hide-all').removeClass('d-none');
});
$('.lessons-list .hide-all').click(function(){
  $(this).closest('.lessons-list').find('li.dd-none').addClass('d-none');
  $(this).addClass('d-none');
  $(this).parent().find('.show-all').removeClass('d-none');
});
$('.file-block:not(".play-video")').click(function(){
  var link = $(this).data('link');
  switch($(this).data('type')) {
    case 'file':
      window.open(link);
      break;
  }
});
