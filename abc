  <script type="text/javascript">
    $(document).ready(function(){
      $('.main-image.player').click(function(){
        $('.lessons-list-container a:first').click();
      });
      $('.lessons-list-container a').click(function(){
        $('.lessons-list-container a').removeClass('playing');
        $(this).addClass('playing');
        var videoId = $(this).data('videoId');
        var that = this;
        $.getJSON('https://www.goldmedalsquared.com/courses/video/' + videoId, {}, function(result){
          if (result.id) {
            function createPlayer(response) {
              var player = '<div class="invalid-id">Invalid ID</div>';
              if (response.id && response.security_token) {
                var height = response.height / (response.width / $('.player-container').find('.image-title-block').width());
                player = '<div class="video"><iframe class="sproutvideo-player" type="text/html" src="https://videos.sproutvideo.com/embed/' + response.id + '/' + response.security_token + '?type=hd&noBigPlay=true&showcontrols=true" width="100%" height="' + height + '" frameborder="0" allowfullscreen></iframe></div>';
              }
              $('.player-container').find('.image-title-block').html(player);
            }
            function selectElement(element) {
              $('.player-container .row-link .play').removeClass('visible');
              $(element).find('.play').addClass('visible');
            }
            createPlayer(result);
            selectElement(that);
          }
        });
      });
      $('.lessons-list .show-all').click(function(){
        $(this).closest('.lessons-list').find('.d-none').addClass('dd-none').removeClass('d-none');

        $(this).closest('.lessons-list').find('.lessons-list-container').addClass('overflow-6');

        $(this).addClass('d-none');
        $(this).parent().find('.hide-all').removeClass('d-none');
      });
      $('.lessons-list .hide-all').click(function(){
        $(this).closest('.lessons-list').find('.dd-none').addClass('d-none');
        $(this).closest('.lessons-list').find('.lessons-list-container').removeClass('overflow-6');
        $(this).addClass('d-none');
        $(this).parent().find('.show-all').removeClass('d-none');
      });


    });
  </script>

</body>
</html>
